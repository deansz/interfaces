  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id ="content">
    <ul id="navbar" >
    <li><%= link_to "about", about_path %></li>
					<li><%= link_to "theory", theory_path %></li>
